      pod 'Charts'
  # Pods for Go Fitness

  target 'Go FitnessTests' do
    inherit! :search_paths

    # Pods for testing
  end

  target 'Go FitnessUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
